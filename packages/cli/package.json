    "rehype-format": "4.0.0",
    "rehype-stringify": "9.0.2",
    "remark-gfm": "3.0.0",
    "remark-parse": "10.0.0",
    "remark-rehype": "10.0.0",
    "remark-stringify": "10.0.0",
    "retext": "8.1.0",
    "to-vfile": "7.2.2",
    "unified": "10.1.0",
    "unified-args": "9.0.2",
    "unist-util-visit": "4.1.0",
