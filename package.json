{
  "name": "root",
  "description": "A collection of tools to render markdown to slides.",
  "version": "1.1.0",
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "test": "yarn workspaces foreach -ipv run test",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "workspaces": [
    "./packages/*"
  ],
  "devDependencies": {
    "husky": "^7.0.0",
    "pinst": "^2.0.0",
    "typescript": "4.4.3"
  },
  "keywords": [
    "slides",
    "markdown",
    "parser",
    "unified",
    "react",
    "static site generator"
  ],
